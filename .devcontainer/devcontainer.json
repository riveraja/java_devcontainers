{
    "name": "Java",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspace/",
    "features": {
        "ghcr.io/devcontainers/features/java:1": {
            "version": "none",
            "installGradle": "true",
            "installMaven": "true"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-gradle"
            ]
        }
    }
}